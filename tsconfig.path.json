{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@service/*": [
        "./src/service/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@interface/*": [
        "./src/interface/*"
      ],
      "~*": [
        "/src/*"
      ]
    }
  }
}